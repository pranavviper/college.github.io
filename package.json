{
    "name": "college-credit-transfer-system",
    "version": "1.0.0",
    "description": "Root package for simple command execution",
    "scripts": {
        "start": "bash run_project.sh",
        "install:all": "npm install --prefix backend && npm install --prefix frontend",
        "build": "npm install --prefix frontend && npm run build --prefix frontend && rm -rf dist && cp -r frontend/dist dist",
        "cap:sync": "npm run cap:sync --prefix frontend",
        "cap:open:android": "npm run cap:open:android --prefix frontend",
        "cap:open:ios": "npm run cap:open:ios --prefix frontend",
        "web:dev": "npm run dev --prefix frontend",
        "api:dev": "npm run dev --prefix backend"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "axios": "^1.13.4",
        "bcryptjs": "^3.0.3",
        "cors": "^2.8.6",
        "dotenv": "^17.2.3",
        "express": "^5.2.1",
        "helmet": "^8.1.0",
        "jsonwebtoken": "^9.0.3",
        "mongoose": "^9.1.5",
        "morgan": "^1.10.1",
        "multer": "^2.0.2",
        "pdfkit": "^0.17.2"
    }
}